{
    "name": "facility-vscode",
    "displayName": "Facility API",
    "description": "Syntax highlighting for Facility Service Definition (fsd) files.",
    "version": "0.0.1",
    "publisher": "davedunkin",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "fsd",
            "aliases": ["Facility Service Definition", "fsd"],
            "extensions": [".fsd"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "fsd",
            "scopeName": "source.fsd",
            "path": "./syntaxes/fsd.tmLanguage.json"
        }]
    }
}
